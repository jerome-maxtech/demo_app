<h1>Listing parts</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Material</th>
    <th>Coating</th>
    <th>Manufacturing process</th>
    <th>Size</th>
    <th>Length</th>
    <th>Vendor</th>
    <th>Price current</th>
    <th>Price last</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @parts.each do |part| %>
  <tr>
    <td><%= part.name %></td>
    <td><%= part.material %></td>
    <td><%= part.coating %></td>
    <td><%= part.manufacturing_process %></td>
    <td><%= part.size %></td>
    <td><%= part.length %></td>
    <td><%= part.vendor_id %></td>
    <td><%= part.price_current %></td>
    <td><%= part.price_last %></td>
    <td><%= link_to 'Show', part %></td>
    <td><%= link_to 'Edit', edit_part_path(part) %></td>
    <td><%= link_to 'Delete', part, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Part', new_part_path %>
